{
    "manifest_version": 3,
    "name": "LinkedIn Profile Enhancer",
    "version": "1.0",
    "description": "Injects a widget into LinkedIn profiles showing company insights.",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["*://*.linkedin.com/in/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  